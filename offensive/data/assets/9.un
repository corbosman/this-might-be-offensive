function pickuplink_set_visibility(){var b=$("div#pickuplink[fileid]");var c=$("#grid-container ul li[fileid]").first();if(b.length==1&&c.length==1){if(b.attr("fileid")==c.attr("fileid")){b.hide()}else{b.show()}var a=$("#grid-container ul li[fileid='"+b.attr("fileid")+"']");if(a.attr("fileid")!=c.attr("fileid")){a.find("div.col").addClass("hilight_row")}return}if($("a#pickUp").length>0){$("a#pickUp").each(function(f,g){var d=$("#grid-container ul li[fileid='"+$(g).attr("fileid")+"']");if(d.attr("fileid")!=c.attr("fileid")){d.find("div.col").addClass("hilight_row")}})}}$(document).ready(function(){pickuplink_set_visibility()});