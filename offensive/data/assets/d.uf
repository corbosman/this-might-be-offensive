function toggle_subscribe(b,a,c){if(b=="subscribe"){c.html("unsubscribe");c.attr({title:"take this file off my 'unread comments' watch list.",href:"/offensive/subscribe.php?fileid="+a+"&un=1"})}else{c.html("subscribe");c.attr({title:"watch this thread for new comments.",href:"/offensive/subscribe.php?fileid="+a})}}function handle_subscribe(f,d,b){d.preventDefault();var c=f.html();function a(){toggle_subscribe(c,b,f)}f.removeAttr("href");if(c=="subscribe"){$.get("/offensive/api.php/subscribe.php",{threadid:b,subscribe:1},a)}else{$.get("/offensive/api.php/subscribe.php",{threadid:b,subscribe:0},a)}}$(document).ready(function(){$("#subscribeLink").click(function(a){handle_subscribe($(this),a,$("#fileid").attr("value"))});$("#unsubscribeLink").click(function(a){handle_subscribe($(this),a,$("#fileid").attr("value"))})});